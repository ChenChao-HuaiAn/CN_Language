// 测试边界情况
模块 边界测试 {
    公开: 
     整数 零值 = 0;
     整数 零运算 = 0 + 0;
     整数 负数 = -100;
     整数 负数运算 = -50 - 50;
     整数 大数 = 999999;
     整数 大数运算 = 1000000 - 1;
     整数 除法 = 100 / 10;
     整数 模运算 = 17 % 5;
}

函数 主程序() {
    整数 v1 = 边界测试.零值;
    整数 v2 = 边界测试.零运算;
    整数 v3 = 边界测试.负数;
    整数 v4 = 边界测试.负数运算;
    整数 v5 = 边界测试.大数;
    整数 v6 = 边界测试.大数运算;
    整数 v7 = 边界测试.除法;
    整数 v8 = 边界测试.模运算;
    返回 0;
}
