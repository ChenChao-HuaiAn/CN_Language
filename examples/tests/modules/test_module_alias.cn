// 模块别名功能测试
// 测试括号语法：导入 模块名(别名);

模块 非常长的模块名称 {
    公开: 整数 数据 = 100;
    公开: 整数 计数器 = 0;
    
    公开: 函数 获取数据() {
        返回 数据;
    }
    
    公开: 函数 计算() {
        返回 数据 * 2;
    }
}

模块 数学工具 {
    公开: 整数 常量 = 42;
    
    公开: 函数 加(整数 a, 整数 b) {
        返回 a + b;
    }
    
    公开: 函数 乘(整数 a, 整数 b) {
        返回 a * b;
    }
}

// 使用别名导入模块
导入 非常长的模块名称(短名);
导入 数学工具(数学);

函数 主程序() {
    // 通过别名访问模块成员
    变量 x = 短名.数据;
    打印整数(x);  // 输出: 100
    
    变量 y = 短名.获取数据();
    打印整数(y);  // 输出: 100
    
    变量 z = 短名.计算();
    打印整数(z);  // 输出: 200
    
    // 通过另一个别名访问
    变量 常数 = 数学.常量;
    打印整数(常数);  // 输出: 42
    
    变量 和 = 数学.加(10, 20);
    打印整数(和);  // 输出: 30
    
    变量 积 = 数学.乘(5, 6);
    打印整数(积);  // 输出: 30
    
    返回 0;
}
