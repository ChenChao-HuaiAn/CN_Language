// 模块别名与选择性导入组合测试
// 测试同时使用别名和选择性导入

模块 配置模块 {
    公开: 整数 版本 = 1;
    公开: 整数 端口 = 8080;
    公开: 字符串 主机 = "localhost";
    
    公开: 函数 获取配置() {
        返回 版本;
    }
}

模块 工具模块 {
    公开: 整数 工具版本 = 2;
    
    公开: 函数 帮助函数A() {
        返回 100;
    }
    
    公开: 函数 帮助函数B() {
        返回 200;
    }
    
    公开: 函数 帮助函数C() {
        返回 300;
    }
}

// 别名导入：不导入成员
导入 配置模块(配置);

// 选择性导入：导入指定成员
导入 工具模块 { 帮助函数A, 帮助函数B };

函数 主程序() {
    // 使用别名访问
    变量 v = 配置.版本;
    打印整数(v);  // 输出: 1
    
    变量 p = 配置.端口;
    打印整数(p);  // 输出: 8080
    
    变量 cfg = 配置.获取配置();
    打印整数(cfg);  // 输出: 1
    
    // 直接使用导入的成员
    变量 a = 帮助函数A();
    打印整数(a);  // 输出: 100
    
    变量 b = 帮助函数B();
    打印整数(b);  // 输出: 200
    
    // 未导入的成员需要通过模块名访问
    变量 c = 工具模块.帮助函数C();
    打印整数(c);  // 输出: 300
    
    // 未导入的成员也可以通过模块名访问工具版本
    变量 tv = 工具模块.工具版本;
    打印整数(tv);  // 输出: 2
    
    返回 0;
}
