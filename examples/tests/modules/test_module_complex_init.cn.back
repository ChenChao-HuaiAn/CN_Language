// 测试模块变量的复杂初始化
模块 数学 {
    整数 基数 = 10;
    整数 加法结果 = 1 + 2;
    整数 乘法结果 = 5 * 3;
    整数 复杂表达式 = (10 + 5) * 2;
    整数 负数 = -100;
}

函数 主程序() {
    打印("基数: ");
    打印(数学.基数);
    打印("\n");
    
    打印("加法结果 (1+2): ");
    打印(数学.加法结果);
    打印("\n");
    
    打印("乘法结果 (5*3): ");
    打印(数学.乘法结果);
    打印("\n");
    
    打印("复杂表达式 ((10+5)*2): ");
    打印(数学.复杂表达式);
    打印("\n");
    
    打印("负数: ");
    打印(数学.负数);
    打印("\n");
    
    返回 0;
}
