函数 主程序() {
    // 运行时全面测试示例（现在支持字符串变量语义）
    打印("  你好, CN_Language!  ");
    打印("\n");
    
    变量 长度值 = 长度("  你好, CN_Language!  ");
    打印("文本长度: ");
    
    // 测试字符串变量：赋值、拷贝、作为参数
    变量 文本 = "  你好, CN_Language!  ";
    变量 修整后的文本 = 文本;
    打印(文本);
    打印("\n");
    变量 文本长度 = 长度(文本);
    
    // 数组字面量测试
    变量 整数数组 = [1, 2, 3, 4, 5];
    变量 数组长度 = 长度(整数数组);
    
    返回 0;
}
