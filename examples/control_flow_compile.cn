函数 主程序()
{
    变量 i = 0;
    变量 sum = 0;
    对于 (; (i <= 10); i = (i + 1)) {
        sum = (sum + i);
    }
    如果 ((sum == 55)) {
        打印("循环测试通过: 55\n");
    } 否则 {
        打印("循环测试失败\n");
    }
    变量 j = 5;
    循环 ((j > 0)) {
        j = (j - 1);
    }
    如果 ((j == 0)) {
        打印("当循环测试通过\n");
    }
    返回 0;
}
