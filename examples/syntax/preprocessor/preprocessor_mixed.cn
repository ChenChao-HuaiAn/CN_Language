// 混合使用中英文预处理器指令示例

// 使用英文定义
#define MAX_SIZE 100
#define MIN_SIZE 10

// 使用中文定义
#定义 数组大小 50
#定义 默认值 42

// 混合条件编译
#ifdef MAX_SIZE
// 英文条件分支
#定义 功能A
#endif

#如果定义 功能A
// 中文条件分支  
#define FEATURE_B
#结束如果

// 函数定义
函数 测试混合宏() {
    变量 max = MAX_SIZE;      // 使用英文宏
    变量 size = 数组大小;      // 使用中文宏
    变量 def = 默认值;         // 使用中文宏
    
    返回 max + size + def;
}

// 主程序
函数 主程序() {
    变量 result = 测试混合宏();
    返回 0;
}
