// 结构体调试测试

// 结构体定义（当前建议在全局作用域定义）
结构体 人员 {
    字符串 姓名;
    整数 年龄;
}

函数 主程序() {
    
    // 创建结构体实例（使用字段名初始化）
    变量 张三 = 人员{姓名: "张三", 年龄: 25};
    
    // 直接打印结构体字段值
    打印("测试开始");
    
    // 访问结构体字段
    变量 姓名值 = 张三.姓名;
    变量 年龄值 = 张三.年龄;
    
    打印("姓名值类型: " + 姓名值);
    打印("年龄值类型: " + 年龄值);
    
    返回 0;
}
