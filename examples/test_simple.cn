// 简单的CN语言测试程序
函数 主程序() {
    打印("=== 字节码执行引擎测试 ===");
    打印("测试1: 基本算术运算");
    
    变量 a = 10;
    整数 b = 5;
    整数 和 = a + b;
    整数 差 = a - b;
    整数 积 = a * b;
    整数 商 = a / b;
    
    打印("a = " + a);
    打印("b = " + b);
    打印("a + b = " + 和);
    打印("a - b = " + 差);
    打印("a * b = " + 积);
    打印("a / b = " + 商);
    
    打印("测试2: 条件语句");
    如果 (a > b) {
        打印("a 大于 b");
    } 否则 {
        打印("a 不大于 b");
    }
    
    打印("测试3: 循环");
    整数 i = 0;
    整数 总和 = 0;
    当 (i < 5) {
        总和 = 总和 + i;
        i = i + 1;
    }
    打印("0到4的和 = " + 总和);
    
    打印("=== 测试完成 ===");
    返回 0;
}
