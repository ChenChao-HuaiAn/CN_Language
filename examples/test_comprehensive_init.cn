// 综合测试：模块变量的复杂初始化
// 测试各种类型和表达式的初始化

模块 数学常量 {
    公开:
    整数 十 = 10;
    整数 百 = 100;
}

模块 基础运算 {
    公开:
    // 字面量初始化
    整数 字面量整数 = 42;
    小数 字面量小数 = 3.14;
    
    // 简单表达式
    整数 加法 = 10 + 20;
    整数 减法 = 50 - 15;
    整数 乘法 = 6 * 7;
    整数 除法 = 100 / 5;
    整数 取模 = 17 % 5;
    
    // 浮点运算
    小数 浮点加法 = 2.5 + 1.5;
    小数 浮点乘法 = 3.0 * 2.5;
}

模块 复杂运算 {
    公开:
    // 嵌套表达式
    整数 嵌套1 = ((5 + 3) * 2) - 1;
    整数 嵌套2 = (100 / (2 + 3)) * 4;
    整数 多层 = (((1 + 2) * 3) + ((4 - 1) * 2));
    
    // 一元运算
    整数 负数 = -100;
    整数 负数运算 = -50 + 25;
    
    // 边界值
    整数 零 = 0;
    整数 大数 = 999999;
}

函数 主程序() {
    // 访问所有初始化的变量
    整数 sum1 = 基础运算.字面量整数 + 基础运算.加法;
    整数 sum2 = 复杂运算.嵌套1 + 复杂运算.嵌套2;
    整数 sum3 = 复杂运算.多层 + 复杂运算.负数运算;
    
    小数 fsum = 基础运算.字面量小数 + 基础运算.浮点加法;
    
    // 测试零和大数
    整数 边界 = 复杂运算.零 + 复杂运算.大数;
    
    返回 0;
}
